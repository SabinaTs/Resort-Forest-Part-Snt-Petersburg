<Window x:Class="CurortPark.PagesAndWindows.Manager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CurortPark.PagesAndWindows"
        mc:Ignorable="d"
        Title="Manager" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="360*"/>
            <RowDefinition Height="59*"/>
        </Grid.RowDefinitions>
        <Button x:Name="edit" Grid.Row="1" HorizontalAlignment="Left" Height="50" Width="300" FontFamily="Comic Sans MS" HorizontalContentAlignment="Center" VerticalAlignment="Stretch" FontSize="14" Content="Редактировать" Click="EditBtn" Margin="19,0,0,9"/>
        <Button x:Name="exit" Grid.Row="1" HorizontalAlignment="Right" Height="50" Width="300" FontFamily="Comic Sans MS" HorizontalContentAlignment="Center" VerticalAlignment="Stretch" FontSize="14" Content="Выйти" Click="ExitBtn" Margin="0,0,19,9"/>
    </Grid>
</Window>
